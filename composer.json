{
    "name": "floatingpointsoftware/stylist",
    "description": "Laravel 4 theming package.",
    "authors": [
        {
            "name": "Kirk Bushell",
            "email": "torm3nt@gmail.com"
        },
        {
            "name": "Mike Dugan",
            "email": "mike@mjdugan.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/html": "~5.0",
        "illuminate/support": "5.0.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "orchestra/testbench": "3.0.*",
        "phpunit/phpunit": "4.2.*",
        "apigen/apigen": "2.8.1",
        "barryvdh/laravel-ide-helper": "1.11.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "FloatingPoint\\Stylist\\": "src/Stylist",
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ]
}
